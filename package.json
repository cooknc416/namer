{
  "name": "namer",
  "version": "1.0.0",
  "description": "A media file renamer built with Electron, React, and Typescript.",
  "repository": "git@github.com:cooknc416/namer.git",
  "author": "Nathan Cook <cooknc416@gmail.com>",
  "license": "MIT",
  "private": true,
  "main": "./dist/main.js",
  "scripts": {
    "start": "NODE_ENV=development webpack --config webpack.electron.config.js --mode development && electron .",
    "dev": "concurrently \"yarn dev:react\" \"yarn dev:electron\"",
    "build": "rimraf ./public && mkdir public && webpack --progress -p",
    "server": "nodemon ./electron/server.ts",
    "dev:electron": "NODE_ENV=development webpack --config webpack.electron.config.js --mode development && electron .",
    "dev:react": "NODE_ENV=development webpack-dev-server --config webpack.react.config.js --mode development",
    "lint": "node_modules/eslint/bin/eslint.js src",
    "test": "node_modules/jest/bin/jest.js"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.8",
    "@types/jest": "^26.0.9",
    "@types/mathjs": "^6.0.5",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "coveralls": "^3.1.0",
    "dotenv-webpack": "^2.0.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-modules-newline": "^0.0.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.3.0",
    "ts-jest": "^26.2.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "date-fns": "^2.15.0",
    "electron": "^9.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "mathjs": "^7.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.3",
    "react-is": "^16.13.1",
    "regenerator-runtime": "^0.13.7",
    "styled-components": "^5.1.1",
    "uuid": "^8.3.0"
  }
}